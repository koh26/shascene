<%= form_with(model: @post, url: new_map_post_path(@city), method: :get, local: true )do |f| %>

  <%= f.text_field :image %>

  <%= f.text_field :title %>

  <%= f.text_area :about %>

  <%= f.date_select :day %>

  <%= f.text_field :city %>

  <%= f.text_field :number %>

  <%= f.text_field :building %>

  <%= f.submit %>

<% end %>

